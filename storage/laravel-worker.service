[Unit]
Description=LARAVEL Worker Service
After=network.target

[Service]
User=www-data
Group=www-data
WorkingDirectory=/home/admin/web/m.easy-order-taxi.site/public_html/
Restart=always
RestartSec=3
ExecStart=/usr/bin/php7.3 artisan queue:work --sleep=3 --tries=3 --timeout=90

[Install]
WantedBy=multi-user.target





